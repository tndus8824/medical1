import requests
from bs4 import BeautifulSoup

url="https://www.google.co.kr/search?q=%EC%9B%90%EB%8B%AC%EB%9F%AC%ED%99%98%EC%9C%A8&sca_esv=125395e02bc1fefe&sca_upv=1&hl=ko&source=hp&ei=kQ0iZuOoDoX91e8P8ay0CA&iflsig=ANes7DEAAAAAZiIboTF68T6cH_9PiewtaXYlH9e9cF66&udm=&ved=0ahUKEwijtZXc0c2FAxWFfvUHHXEWDQEQ4dUDCA8&uact=5&oq=%EC%9B%90%EB%8B%AC%EB%9F%AC%ED%99%98%EC%9C%A8&gs_lp=Egdnd3Mtd2l6Ig_-&sclient=gws-wiz"
headers={"user-Agent":"Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/123.0.0.0 Safari/537.36"}
res=requests.get(url,headers=headers)
res.raise_for_status()

soup=BeautifulSoup(res.text,"lxml")
with open('google2.html','w',encoding="utf8") as f:
    f.write(soup.prettify())
    
#타이틀출력

title=soup.find("input","lWzCpb ZEB7Fb")
print(soup.title.text)
#해당태그를 출력
print("미국 달러: ",soup.find("input",{"class":"lWzCpb ZEB7Fb"}))
#해당태그의 속성값 모두 출력
print("미국 달러: ",soup.find("input",{"class":"lWzCpb ZEB7Fb"}).attrs)

#해당태그의 특정속성값 1개 출력
print("미국 달러: ",soup.find("input",{"class":"lWzCpb ZEB7Fb"})["value"])

#대한민국 원
print("대한민국 원: ",soup.find("input",{"class":"lWzCpb a61j6"})["value"])


